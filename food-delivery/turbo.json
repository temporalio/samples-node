{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "env": ["NODE_ENV", "TEMPORAL_SERVER", "NAMESPACE", "TEMPORAL_CLOUD_CERT", "TEMPORAL_CLOUD_KEY"],
      "outputs": ["lib/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start": {}
  }
}
